@model List<NoteViewModels.Preview>

@if (Model.Count > 0)
{
    <table>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Content Preview</th>
        </tr>
        @foreach (NoteViewModels.Preview note in Model)
        {
            <tr>
                <td>@note.Id</td>
                <td>@note.Title</td>
                <td>@note.ContentPreview</td>
            </tr>
        }
    </table>
}
else
{
    <p>No results</p>
}